{
  "private": true,
  "config": {
    "path": {
      "src": "tailoff",
      "public": "public"
    },
    "proxy": ""
  },
  "scripts": {
    "dev": "node_modules/webpack/bin/webpack.js --env.NODE_ENV=development --progress --hide-modules",
    "watch": "npm run dev -- --watch",
    "dev-purge": "node_modules/webpack/bin/webpack.js --env.NODE_ENV=development --env.purge=true --progress --hide-modules",
    "watch-purge": "npm run dev-purge -- --watch",
    "prod": "node_modules/webpack/bin/webpack.js --env.NODE_ENV=production --no-progress --hide-modules",
    "clean": "rm -rf public/css public/docs public/fonts public/img public/js public/**/*.js public/**/*.css"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "2.24.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.1.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "4.19.1",
    "eslint-loader": "^2.1.1",
    "favicons-webpack-plugin": "^3.0.1",
    "globby": "^10.0.1",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "icon-font-loader": "^2.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-cli": "^7.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "purgecss-webpack-plugin": "^1.6.0",
    "style-loader": "^0.23.1",
    "tailwindcss": "^1.2.0",
    "tailwindcss-aspect-ratio": "^3.0.0",
    "terser-webpack-plugin": "^1.3.0",
    "webfonts-loader": "ariesjia/webfonts-loader",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6"
  },
